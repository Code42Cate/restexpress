		"@babel/preset-env": "^7.10.1",
		"babel-plugin-root-import": "^6.5.0",
		"eslint": "^6.8.0",
		"mongodb-memory-server": "^6.6.1",
		"nodemon": "^2.0.4",
		"nyc": "^15.0.1",
		"supertest": "^4.0.2"
